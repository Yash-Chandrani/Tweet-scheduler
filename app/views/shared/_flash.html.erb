<% if flash[:notice]%> 
<%# <% without an = sign means we don't want to print the return value %>
    <div class="alert alert-primary mt-4" role="alert">
    <%=flash[:notice]%>
    </div>
<%end%>

<% if flash[:alert]%> 
    <div class="alert alert-secondary mt-4" role="alert">
    <%=flash[:alert]%>
    </div>
<%end%>

